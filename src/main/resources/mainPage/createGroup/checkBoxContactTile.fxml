<HBox alignment="CENTER_LEFT" minHeight="-Infinity" minWidth="-Infinity" prefHeight="80.0" prefWidth="230.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.CheckboxContactTileController">
   <children>
      <ImageView fx:id="image" fitHeight="70.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true" />
      <VBox alignment="CENTER_LEFT" prefHeight="70.0" prefWidth="129.0">
         <children>
            <Label fx:id="name" maxHeight="35.0" prefHeight="20.0" prefWidth="130.0" text="Shivain Kumar" wrapText="true">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="jobTitle" maxHeight="35.0" prefHeight="17.0" prefWidth="135.0" text="Senior gym bro" />
         </children>
         <HBox.margin>
            <Insets right="5.0" />
         </HBox.margin>
      </VBox>
      <CheckBox fx:id="checkbox" mnemonicParsing="false" />
   </children>
</HBox>
